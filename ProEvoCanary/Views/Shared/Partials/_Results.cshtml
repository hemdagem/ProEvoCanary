@model List<ProEvoCanary.Models.ResultsModel>
<div class="block Results">
    <h2>Results</h2>
    <table>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.HomeTeam

                </td>
                <td>
                    @item.HomeScore
                </td>
                <td width='10' align='center'>
                    -
                </td>
                <td>
                    @item.AwayScore
                </td>
                <td>
                    @item.AwayTeam
                </td>
                <td>
                    <a href="@Url.Action("Details","Results",new{ Id=@item.ResultID})">View Match Information</a>

                </td>
            </tr>
        }
    </table>
</div>