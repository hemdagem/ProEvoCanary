@model List<ProEvo45.Models.RssFeedModel>
News

<table width="95%" cellpadding="0" cellspacing="0" style="text-align: center; margin-left: 5px;">
    @foreach (var item in Model.Take(5))
    {

        <tr id="Tr3"style="width: 66px;">
            <td rowspan="3" style="border-bottom: solid 1px white;">

                <a target="new" href="@item.LinkUrl">
                    <img src="@item.LinkTitle" />
                </a>
            </td>
        </tr>
        <tr id="Tr1">
            <td>
                <a target="new" href="@item.LinkUrl">@item.LinkTitle</a>
            </td>
        </tr>
        <tr id="Tr2">
            <td style="border-bottom: solid 1px white;">
                @item.LinkDescription
            </td>
        </tr>

    }
</table>