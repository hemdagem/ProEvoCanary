@model List<ProEvoCanary.Web.Models.ResultsModel>

@if (Model.Count > 0)
{
    <table class="centered">
        @foreach (var item in Model)
        {
            <tr>
                <td class="center-align">@item.HomeTeam</td>
                <td class="center-align">@item.HomeScore - @item.AwayScore</td>
                <td class="center-align">@item.AwayTeam</td>
            </tr>
        }
    </table>
}